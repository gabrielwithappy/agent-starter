{
	"servers": {
		"google-workspace-mcp": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"-y",
				"@taylorwilsdon/google_workspace_mcp"
			]
		},
		"microsoft/markitdown": {
			"type": "stdio",
			"command": "uvx",
			"args": [
				"markitdown-mcp==0.0.1a4"
			],
			"gallery": "https://api.mcp.github.com",
			"version": "1.0.0"
		},
		"com.supabase/mcp": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"--project-ref",
				"${input:project-ref}",
				"--read-only",
				"${input:read-only}",
				"--features",
				"${input:features}",
				"--api-url",
				"${input:api-url}",
				"@supabase/mcp-server-supabase@0.6.3"
			],
			"env": {
				"SUPABASE_ACCESS_TOKEN": "${input:SUPABASE_ACCESS_TOKEN}"
			},
			"gallery": "https://api.mcp.github.com",
			"version": "0.6.3"
		},
		"io.github.vercel/next-devtools-mcp": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"next-devtools-mcp@0.3.6"
			],
			"gallery": "https://api.mcp.github.com",
			"version": "0.3.6"
		},
		"desktop-commander": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"-y",
				"@wonderwhy-er/desktop-commander"
			]
		}
	},
	"inputs": [
		{
			"id": "project-ref",
			"type": "promptString",
			"description": "Supabase project reference ID",
			"password": false
		},
		{
			"id": "read-only",
			"type": "promptString",
			"description": "Enable read-only mode",
			"password": false
		},
		{
			"id": "features",
			"type": "promptString",
			"description": "Comma-separated list of features to enable",
			"password": false
		},
		{
			"id": "api-url",
			"type": "promptString",
			"description": "Custom API URL",
			"password": false
		},
		{
			"id": "SUPABASE_ACCESS_TOKEN",
			"type": "promptString",
			"description": "Personal access token for Supabase API",
			"password": true
		}
	]
}