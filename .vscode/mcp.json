{
	"servers": {
		"playwright": {
			"command": "npx",
			"args": [
				"@playwright/mcp@latest"
			]
		},
		"n8n-mcp": {
			"command": "npx",
			"args": [
				"n8n-mcp"
			],
			"env": {
				"MCP_MODE": "stdio",
				"LOG_LEVEL": "error",
				"DISABLE_CONSOLE_OUTPUT": "true",
				"N8N_API_URL": "http://localhost:5678",
			}
		},
		"Framelink Figma MCP": {
			"command": "cmd",
			"args": [
				"/c",
				"npx",
				"-y",
				"figma-developer-mcp",
				"--figma-api-key=%FIGMA_API_KEY%",
				"--stdio"
			],
			"type": "stdio"
		},
		"TalkToFigma": {
			"command": "bunx",
			"args": [
				"cursor-talk-to-figma-mcp@latest",
				"--figma-api-key=%FIGMA_API_KEY%",
				"--channel=%FIGMA_CHANNEL%"
			],
			"type": "stdio"
		},
		"Memory MCP": {
			"command": "npx",
			"args": [
				"-y",
				"@modelcontextprotocol/server-memory"
			]
		},
		"Puppeteer MCP": {
			"command": "cmd",
			"args": [
				"/c",
				"npx",
				"-y",
				"@modelcontextprotocol/server-puppeteer@latest"
			],
			"type": "stdio"
		}
	},
	"inputs": []
}